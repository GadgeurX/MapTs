<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:mapbox="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".activity.MapActivity">

    <com.mapbox.mapboxsdk.maps.MapView
            android:id="@+id/mapView"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            mapbox:mapbox_cameraBearing="-17.6"
            mapbox:mapbox_cameraTargetLat="47.0912337940309"
            mapbox:mapbox_cameraTargetLng="-0.8769057444109194"
            mapbox:mapbox_cameraTilt="45"
            mapbox:mapbox_cameraZoom="7.0"
            mapbox:mapbox_uiDoubleTapGestures="true"
            mapbox:mapbox_uiHorizontalScrollGestures="true"
            mapbox:mapbox_uiQuickZoomGestures="true"
            mapbox:mapbox_uiRotateGestures="true"
            mapbox:mapbox_uiScrollGestures="true"
            mapbox:mapbox_uiTiltGestures="true"
            mapbox:mapbox_uiZoomGestures="true"
            mapbox:mapbox_uiCompass="true"/>

</androidx.constraintlayout.widget.ConstraintLayout>